@{
    ViewData["Title"] = "Friend List";
    Layout = "~/Views/GeneralLayout.cshtml";
}

<h1>FriendList</h1>



<div id="friendBody">

</div>


<script id="friend-template" type="text/x-handlebars-template">
    <div class="friendDiv">
        <a href="/Profile/Profile?userId={{Id}}">
            {{#if ProfilePhoto}}
            <img src="/Photos/Download/{{ProfilePhoto}}"  />
            {{else}}
            <img src="/images/DefaultProfile.png"  />
            {{/if}}
            <p>{{Name}}</p>


        </a>
    </div>
</script>
<script src="~/js/Friends.js"></script>